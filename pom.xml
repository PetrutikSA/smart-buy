		<artifactId>spring-boot-starter-parent</artifactId>
		<version>3.4.1</version>
		<relativePath/> <!-- lookup parent from repository -->
	</parent>
	<groupId>ru.petrutik</groupId>
	<artifactId>smart-buy</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>smart-buy</name>
	<description>Telegram bot to check prices in web markets everyday</description>
	<modules>
		<module>smart-buy-gateway</module>
	</modules>
	<packaging>pom</packaging>
	<url/>
